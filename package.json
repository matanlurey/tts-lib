{
  "name": "@matanlurey/tts-runner",
  "version": "0.1.0",
  "description": "Node.js library for running Tabletop Simulator",
  "scripts": {
    "build": "tsc --build tsconfig.dist.json && ts-node tool/postbuild.ts",
    "fix": "prettier --write . --ignore-path .gitignore && eslint --fix src test --ext .ts",
    "lint": "prettier --ignore-path .gitignore --list-different src && eslint src test --ext .ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matanlurey/tts-runner.git"
  },
  "main": "index.ts",
  "types": "index.d.ts",
  "author": "Matan Lurey",
  "keywords": [
    "Tabletop",
    "Simulator",
    "TTS",
    "Runner",
    "Launcher",
    "CLI"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.0",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
